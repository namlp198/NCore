            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid x:Name="PART_SliderMaxMin"
                      Grid.Row="0"
                      Width="{TemplateBinding Width}"
                      Height="Auto">
                    <TextBlock Text="{TemplateBinding Minimum, Converter={StaticResource ValueToStringConverter}}"
                               HorizontalAlignment="Left" />
                    <TextBlock Text="{TemplateBinding MediumValue, Converter={StaticResource ValueToStringConverter}}"
                               HorizontalAlignment="Center" />
                    <TextBlock Text="{TemplateBinding Maximum, Converter={StaticResource ValueToStringConverter}}"
                               HorizontalAlignment="Right" />
                </Grid>
                <Grid Grid.Row="1"
                      Width="{TemplateBinding Width}"
                      Background="Transparent">
                    <Separator Grid.Column="1"
                               Width="16"
                               Height="1"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Bottom">
                        <Separator.RenderTransform>
                            <TransformGroup>
                                <RotateTransform Angle="90" />
                                <TranslateTransform X="8"
                                                    Y="2" />
                            </TransformGroup>
                        </Separator.RenderTransform>
                    </Separator>
                </Grid>
                <Grid x:Name="PART_SliderContainer"
                      Grid.Row="2"
                      Background="Transparent"
                      Width="{TemplateBinding Width}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Border x:Name="PART_StartLength"
                                Grid.Column="0"
                                Background="{TemplateBinding UnHighlightBackground}"
                                Height="{TemplateBinding Thickness}"
                                Width="{TemplateBinding Width, Converter={StaticResource PercententageConvert}, ConverterParameter=0.5}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center" />
                        <Border x:Name="PART_MiddleLength"
                                Grid.Column="1"
                                Background="{TemplateBinding HighlightBackground}"
                                Height="{TemplateBinding Thickness}"
                                Width="{TemplateBinding Width, Converter={StaticResource PercententageConvert}, ConverterParameter=0.1}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center" />
                        <Border x:Name="PART_EndLength"
                                Grid.Column="2"
                                Background="{TemplateBinding UnHighlightBackground}"
                                Height="{TemplateBinding Thickness}"
                                Width="{TemplateBinding Width, Converter={StaticResource PercententageConvert}, ConverterParameter=0.4}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center" />
                        <Thumb x:Name="PART_StartThumb"
                               Grid.Column="0"
                               HorizontalAlignment="Right"
                               Style="{TemplateBinding StartThumbStyle}"
                               ToolTip="{TemplateBinding StartThumbToolTip}" />
                        <Thumb x:Name="PART_EndThumb"
                               Grid.Column="2"
                               HorizontalAlignment="Left"
                               Style="{TemplateBinding EndThumbStyle}"
                               ToolTip="{TemplateBinding EndThumbToolTip}" />
                    </Grid>
                    <Grid Grid.Row="1"
                          Margin="0,5,0,0"
                          Width="{TemplateBinding Width}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Border x:Name="PART_StartValueArea"
                                Grid.Column="0"
                                MaxWidth="{TemplateBinding Width}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center">
                            <StackPanel Width="30"
                                        Margin="-15,0,-15,0">
                                <TextBlock x:Name="PART_StartLabel"
                                           Text="{Binding Start, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, 
                                              RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}"
                                           TextAlignment="Center"
                                           HorizontalAlignment="Center" />
                            </StackPanel>
                        </Border>
                        <Border x:Name="PART_EndValueArea"
                                Grid.Column="1"
                                MaxWidth="{TemplateBinding Width}"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Center">
                            <StackPanel Width="30"
                                        Margin="-15,0,-15,0">
                                <TextBlock Text="{Binding End, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,
                                              RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}"
                                           TextAlignment="Center"
                                           HorizontalAlignment="Center" />
                            </StackPanel>
                        </Border>
                    </Grid>
                </Grid>

            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled"
                     Value="False">
                <Setter Property="Opacity"
                        Value="0.5" />
                <Setter TargetName="PART_MiddleLength"
                        Property="Background"
                        Value="{Binding ElementName=PART_StartLength, Path=Background}" />
            </Trigger>
            <Trigger Property="IsReadOnly"
                     Value="True">
                <Setter Property="Opacity"
                        Value="0.65" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

