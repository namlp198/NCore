    <ControlTemplate TargetType="{x:Type Control:SliderControl}"
                     x:Key="TwoSliderControlHasInputTemplate">
        <Border BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Background="{TemplateBinding Background}"
                Width="{TemplateBinding Width}"
                Height="{TemplateBinding Height}"
                Padding="{TemplateBinding Padding}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid x:Name="PART_SliderMaxMin"
                      Grid.Row="0"
                      Width="{TemplateBinding Width}"
                      Height="Auto">
                    <TextBlock Text="{TemplateBinding Minimum, Converter={StaticResource ValueToStringConverter}}"
                               HorizontalAlignment="Left" />
                    <TextBlock Text="{TemplateBinding Maximum, Converter={StaticResource ValueToStringConverter}}"
                               HorizontalAlignment="Right" />
                </Grid>
                <Grid x:Name="PART_SliderContainer"
                      Grid.Row="1"
                      Background="Transparent">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Border x:Name="PART_StartLength"
                            Grid.Column="0"
                            Background="{TemplateBinding UnHighlightBackground}"
                            Height="{TemplateBinding Thickness}"
                            Width="{TemplateBinding Width, Converter={StaticResource PercententageConvert}, ConverterParameter=0.5}"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center" />
                    <Border x:Name="PART_MiddleLength"
                            Grid.Column="1"
                            Background="{TemplateBinding HighlightBackground}"
                            Height="{TemplateBinding Thickness}"
                            Width="{TemplateBinding Width, Converter={StaticResource PercententageConvert}, ConverterParameter=0.1}"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center" />
                    <Border x:Name="PART_EndLength"
                            Grid.Column="2"
                            Background="{TemplateBinding UnHighlightBackground}"
                            Height="{TemplateBinding Thickness}"
                            Width="{TemplateBinding Width, Converter={StaticResource PercententageConvert}, ConverterParameter=0.4}"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center" />
                    <Thumb x:Name="PART_StartThumb"
                           Grid.Column="0"
                           HorizontalAlignment="Left"
                           Style="{TemplateBinding StartThumbStyle}"
                           ToolTip="{TemplateBinding StartThumbToolTip}" />
                    <Thumb x:Name="PART_EndThumb"
                           Grid.Column="2"
                           HorizontalAlignment="Right"
                           Style="{TemplateBinding EndThumbStyle}"
                           ToolTip="{TemplateBinding EndThumbToolTip}" />
                </Grid>
                <Grid Grid.Row="2"
                      Width="{TemplateBinding Width}"
                      Height="Auto">
                    <StackPanel Orientation="Horizontal"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Margin="0,20,0,0">
                        <TextBlock Text="Min."
                                   VerticalAlignment="Center"
                                   Margin="0,0,5,0" />
                        <Control:TextBoxControl x:Name="PART_MinValue"
                                                Width="50"
                                                Height="25"
                                                CornerRadius="2"
                                                HorizontalContentAlignment="Right"
                                                VerticalAlignment="Center"
                                                Text="{Binding Start, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, 
                                                       RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}, 
                                                       NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                Style="{StaticResource TextBoxWithPlaceHolderStyle}"
                                                Validation.ErrorTemplate="{StaticResource ValidatedTextBoxTemplate}"
                                                FontSize="{TemplateBinding FontSize}"
                                                Foreground="{TemplateBinding Foreground}"
                                                FontStyle="{TemplateBinding FontStyle}"
                                                Background="{TemplateBinding InputBackground}"
                                                BorderBrush="{TemplateBinding InputBorderBrush}"
                                                BorderThickness="1">
                        </Control:TextBoxControl>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Center"
                                Margin="0,20,0,0">
                        <TextBlock Text="Max."
                                   VerticalAlignment="Center"
                                   Margin="0,0,5,0" />
                        <Control:TextBoxControl x:Name="PART_MaxValue"
                                                Width="50"
                                                Height="25"
                                                CornerRadius="2"
                                                HorizontalContentAlignment="Right"
                                                VerticalAlignment="Center"
                                                Text="{Binding End, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, 
                                                       RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}"
                                                Style="{StaticResource TextBoxWithPlaceHolderStyle}"
                                                Validation.ErrorTemplate="{StaticResource ValidatedTextBoxTemplate}"
                                                FontSize="{TemplateBinding FontSize}"
                                                Foreground="{TemplateBinding Foreground}"
                                                FontStyle="{TemplateBinding FontStyle}"
                                                Background="{TemplateBinding InputBackground}"
                                                BorderBrush="{TemplateBinding InputBorderBrush}"
                                                BorderThickness="1" />
                    </StackPanel>
                </Grid>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled"
                     Value="False">
                <Setter Property="Opacity"
                        Value="0.5" />
                <Setter TargetName="PART_MiddleLength"
                        Property="Background"
                        Value="{Binding ElementName=PART_StartLength, Path=Background}" />
            </Trigger>
            <Trigger Property="IsReadOnly"
                     Value="True">
                <Setter Property="Opacity"
                        Value="0.65" />
                <Setter TargetName="PART_MinValue"
                        Property="IsReadOnly"
                        Value="True" />
                <Setter TargetName="PART_MaxValue"
                        Property="IsReadOnly"
                        Value="True" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate TargetType="{x:Type Control:SliderControl}"
                     x:Key="TwoSliderControlTemplate">
