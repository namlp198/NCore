                    xmlns:Control="clr-namespace:NpcCore.Wpf.Controls"
                    xmlns:ControlEx="clr-namespace:NpcCore.Wpf.Ex"
                    xmlns:Converters="clr-namespace:NpcCore.Wpf.Converters">
    <!--<ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Styles/TextBoxStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>-->
    
    <Converters:PercentageConverter x:Key="PercententageConvert" />
    <Converters:ValueToStringConverter x:Key="ValueToStringConverter" />
    <Converters:MarginThumbControlConverter x:Key="MarginThumbControlConverter" />
    <BooleanToVisibilityConverter x:Key="BoolToVis" />
    <Converters:InvertableBooleanToVisibilityConverter x:Key="InvertBoolToVis" />
    
    <Style TargetType="Thumb"
           x:Key="SliderThumbStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <Grid x:Name="OutBox"
                          Width="{Binding Path=ThumbSize, RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}"
                          Height="{Binding Path=ThumbSize, RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}"
                          Margin="{Binding Path=ThumbSize, RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}, 
                                   Converter={StaticResource MarginThumbControlConverter}, ConverterParameter=Horizontal}">
                        <Ellipse Fill="{Binding HighlightBackground, RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}" />
                        <Ellipse x:Name="InBox"
                                 Fill="#373d4e"
                                 Margin="{Binding Path=ThicknessThumb, RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <DataTrigger Binding="{Binding Path=Orientation, RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}}"
                                     Value="Vertical">
                            <Setter Property="Margin"
                                    TargetName="OutBox"
                                    Value="{Binding Path=ThumbSize, RelativeSource={RelativeSource AncestorType={x:Type Control:SliderControl}}, 
                                    Converter={StaticResource MarginThumbControlConverter}, ConverterParameter=Vertical}" />
                        </DataTrigger>
                        <Trigger Property="IsMouseOver"
                                 Value="True">
                            <Setter TargetName="InBox"
                                    Property="Fill"
                                    Value="#464b56" />
                        </Trigger>
                        <Trigger Property="IsDragging"
                                 Value="True">
                            <Setter TargetName="InBox"
                                    Property="Fill"
                                    Value="#ccd8eb" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

